<%= provide(:title, "Sign Up") %>
<h1>Sign Up</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@user) do |f| %>

      <%= render 'form', f: f  %>

      <%= f.submit "Create my account", class: "btn btn-large btn-primary" %>
    <% end %>

    <p> Already have account? <%= link_to "Sign In!", signin_path %></p>
  </div>
</div>
