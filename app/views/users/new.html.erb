<%= provide(:title, "Sign Up") %>
<h1>Sign Up</h1>

<div class="row">
  <div class="span6 offset3">

    <%= render 'shared/error_messages', object: @user %>

    <div class="form-body">
      <%= form_for(@user) do |f| %>

        <%= render 'form', f: f  %>

        <%= f.submit "Create my account", class: "btn btn-block btn-primary" %>
      <% end %>

      <p> Already have account? <%= link_to "Sign In!", signin_path %></p>
    </div>
  </div>
</div>
